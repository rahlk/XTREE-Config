\mode<presentation>

\definecolor{darkblue}{rgb}{0,0,0.8}
\definecolor{wolfred}{rgb}{1,0,0}

\setbeamercolor{alerted text}{fg=darkblue!80!yellow}
\setbeamercolor*{palette primary}{fg=darkblue,bg=white}
\setbeamercolor*{palette secondary}{fg=darkblue!70!black,bg=white}
\setbeamercolor*{palette tertiary}{bg=darkblue!25!white,fg=white}
\setbeamercolor*{palette quaternary}{fg=darkblue,bg=white}

\setbeamercolor*{sidebar}{fg=white,bg=wolfred}

\setbeamercolor*{palette sidebar primary}{fg=darkblue!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=darkblue!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{bg=white}
\setbeamercolor{frametitle right}{bg=white}
\setbeamercolor{frametitle left}{fg=white,bg=wolfred}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
